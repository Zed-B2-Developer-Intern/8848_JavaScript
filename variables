let name="Roshini";
let age=20;
let _isStudent=true;
let $address;
let _role=null;
let cgpa=8.2;
console.log("Name:", name,"Type:",typeof(name));
console.log("Age:", age,"Type:",typeof(age));
console.log("Is Student:", _isStudent,"Type:",typeof(_isStudent));
